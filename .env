# Zoom Credentials
ZOOM_CLIENT_ID=your_zoom_client_id_here
ZOOM_CLIENT_SECRET=your_zoom_client_secret_here
ZOOM_BOT_JID=your_zoom_bot_jid_here
ZOOM_ACCOUNT_ID=your_zoom_account_id_here

# Snowflake Credentials
SNOWFLAKE_ACCOUNT=your_snowflake_account_name_here   # e.g., abc-xy12345
RSA_PRIVATE_KEY_PATH=./path_to_your_private_key.pem    # relative or absolute path to your .pem private key file
SNOWFLAKE_AGENT_ENDPOINT=https://your_agent_endpoint_here   # full URL, e.g., https://xyz.snowflakecomputing.com/agent
SNOWFLAKE_INFERENCE_ENDPOINT=https://your_inference_endpoint_here  # full URL, e.g., https://xyz.snowflakecomputing.com/inference

# Snowflake Tools (Agents use these)
SUPPORT_SEMANTIC_MODEL=your_semantic_model_id_here     # for cortex_analyst_text_to_sql
VEHICLE_SEARCH_SERVICE=your_search_service_id_here     # for cortex_search

# Express Server
PORT=5000    # or whatever port you want (optional, defaults to 5000)
